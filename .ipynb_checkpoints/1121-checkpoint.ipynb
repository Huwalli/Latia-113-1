   "source": [
    "import requests\n",
    " url = '要爬的網址'\n",
    " # 假的headers 資訊\n",
    "headers = {'user-agent': 'Mozilla/5.0 (Windows NT 6.1) AppleWebKit/537.36 \n",
    "(KHTML, like Gecko) Chrome/52.0.2743.116 Safari/537.36'}\n",
    " # 加入headers 資訊\n",
    "web = requests.get(url, headers=headers)\n",
    " web.encoding = 'utf8‘  \n",
    "print(web.text)\n"
   ]
  }
